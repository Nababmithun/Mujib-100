<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/activity_controller"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/bg_color"
        android:orientation="vertical">

        <include
            layout="@layout/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize" />

        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
            android:id="@+id/mediaCoverageSwipeLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/toolbar">

            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/toolbar">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="200dp"
                            android:orientation="vertical">

                            <com.smarteist.autoimageslider.SliderView
                                android:id="@+id/imageSlider"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                app:sliderAnimationDuration="600"
                                app:sliderAutoCycleDirection="back_and_forth"
                                app:sliderAutoCycleEnabled="true"
                                app:sliderCircularHandlerEnabled="true"
                                app:sliderIndicatorAnimationDuration="600"
                                app:sliderIndicatorGravity="center_horizontal|bottom"
                                app:sliderIndicatorMargin="16dp"
                                app:sliderIndicatorOrientation="horizontal"
                                app:sliderIndicatorPadding="3dp"
                                app:sliderIndicatorRadius="1.2dp"
                                app:sliderIndicatorSelectedColor="@color/gold_color"
                                app:sliderIndicatorUnselectedColor="@color/gold_color"
                                app:sliderScrollTimeInSec="1"
                                app:sliderStartAutoCycle="true" />

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="200dp"
                            android:orientation="vertical">

                            <Button
                                android:visibility="gone"
                                android:onClick="playBtn"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"/>

                            <view
                                android:id="@+id/youtube_player"
                                android:background="@drawable/mujib"
                                class="com.google.android.youtube.player.YouTubePlayerView"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent" />

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>



                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="8dp"
                            android:layout_marginRight="8dp"
                            android:orientation="vertical">


                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="16dp"
                                android:fontFamily="@font/lato_regular"
                                android:text="@string/social_feeds"
                                android:textColor="@color/black"
                                android:textSize="@dimen/seventinth_text_size"
                                android:textStyle="normal"
                                tools:ignore="MissingPrefix" />

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="12dp">

                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/socialFeedRV"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_centerInParent="true" />

                            </RelativeLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="16dp"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/newsFeedViewAllTV"
                                    fontPath="fonts/solaiman_lipi.ttf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:fontFamily="@font/lato_regular"
                                    android:onClick="goNewsDetails"
                                    android:text="@string/view_all"
                                    android:textColor="@color/gold_color"
                                    android:textSize="17dp"
                                    tools:ignore="MissingPrefix" />

                                <ImageView
                                    android:layout_width="20dp"
                                    android:layout_height="17dp"
                                    android:layout_marginBottom="2dp"
                                    android:layout_gravity="center_vertical"
                                    android:layout_marginLeft="12dp"
                                    android:src="@drawable/right" />
                            </LinearLayout>

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="200dp">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/tour" />

                                <LinearLayout
                                    android:layout_width="wrap_content"
                                    android:layout_height="200dp"
                                    android:background="#31000000">

                                    <ImageView
                                        android:layout_width="match_parent"
                                        android:layout_height="100dp"
                                        android:layout_gravity="center_vertical"
                                        android:src="@drawable/image_360"
                                        android:tint="#A5A4A2" />
                                </LinearLayout>

                            </RelativeLayout>

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="12dp"
                                android:fontFamily="@font/lato_regular"
                                android:text="@string/bangabandhu_memorial_museum"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/seventinth_text_size"
                                android:textStyle="bold"
                                tools:ignore="MissingPrefix" />

                            <TextView
                                android:id="@+id/virtualTourTV"
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="match_parent"
                                android:layout_height="37dp"
                                android:layout_gravity="center"
                                android:layout_marginLeft="36dp"
                                android:layout_marginTop="4dp"
                                android:layout_marginRight="36dp"
                                android:layout_marginBottom="16dp"
                                android:background="@drawable/btn_bgg"
                                android:fontFamily="@font/lato_regular"
                                android:gravity="center"
                                android:paddingLeft="8dp"
                                android:paddingTop="5dp"
                                android:paddingRight="8dp"
                                android:paddingBottom="5dp"
                                android:text="@string/virtual_tour"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/description_text_size"
                                tools:ignore="MissingPrefix" />
                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:id="@+id/timeLineCard"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginRight="8dp"
                        android:onClick="clickTimeline"
                        app:cardCornerRadius="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <ImageView
                                android:layout_width="match_parent"
                                android:layout_height="200dp"
                                android:scaleType="centerCrop"
                                android:src="@drawable/mujib_timeline" />

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_gravity="center"
                                android:layout_marginTop="8dp"
                                android:fontFamily="@font/lato_regular"
                                android:text="@string/mujib_name"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/seventinth_text_size"
                                tools:ignore="MissingPrefix" />

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginBottom="16dp"
                                android:fontFamily="@font/lato_regular"
                                android:text="@string/timeline"
                                android:textColor="@color/gold_color"
                                android:textSize="@dimen/seventinth_text_size"
                                tools:ignore="MissingPrefix" />
                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginRight="8dp"
                        app:cardCornerRadius="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:id="@+id/playVideoRL"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/varson_image" />

                                <ImageView
                                    android:layout_width="50dp"
                                    android:layout_height="50dp"
                                    android:layout_centerInParent="true"
                                    android:src="@drawable/play"
                                    android:tint="@color/gold_color" />
                            </RelativeLayout>


                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="16dp"
                                android:fontFamily="@font/lato_regular"
                                android:text="@string/speeches"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/seventinth_text_size"
                                android:textStyle="bold"
                                tools:ignore="MissingPrefix" />

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginLeft="4dp"
                                android:layout_marginTop="12dp"
                                android:layout_marginRight="4dp"
                                android:layout_marginBottom="16dp"
                                android:fontFamily="@font/lato_regular"
                                android:gravity="center"
                                android:lineSpacingExtra="4dp"
                                android:text="@string/speeeches_text"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/description_text_size"
                                tools:ignore="MissingPrefix" />

                            <!--<Button
                                android:visibility="gone"
                                android:layout_marginTop="8dp"
                                android:background="@drawable/btn_bg"
                                android:layout_width="match_parent"
                                android:layout_marginRight="18dp"
                                android:layout_marginLeft="18dp"
                                android:text="Watch Now"
                                android:textAllCaps="false"
                                android:layout_marginBottom="8dp"
                                android:layout_height="wrap_content"/>-->

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="16dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginBottom="8dp"
                        app:cardCornerRadius="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="16dp"
                                android:layout_marginTop="12dp"
                                android:fontFamily="@font/lato_regular"
                                android:maxLines="2"
                                android:text="@string/recognising_bgbdu"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/seventinth_text_size"
                                android:textStyle="bold"
                                tools:ignore="MissingPrefix" />

                            <androidx.recyclerview.widget.RecyclerView
                                android:id="@+id/recognizeRecyclerView"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="16dp" />

                            <TextView
                                android:id="@+id/showRecognize"
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center_horizontal"
                                android:layout_marginLeft="6dp"
                                android:layout_marginTop="16dp"
                                android:layout_marginBottom="16dp"
                                android:background="@drawable/btn_bgg"
                                android:fontFamily="@font/lato_regular"
                                android:gravity="center_vertical"
                                android:onClick="showRecognize"
                                android:paddingLeft="12dp"
                                android:paddingTop="7dp"
                                android:paddingRight="12dp"
                                android:paddingBottom="5dp"
                                android:text="@string/view_all"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/description_text_size"
                                tools:ignore="MissingPrefix" />

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginBottom="8dp"
                        app:cardCornerRadius="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="16dp"
                                android:layout_marginTop="12dp"
                                android:fontFamily="@font/lato_regular"
                                android:gravity="center"
                                android:maxLines="2"
                                android:text="@string/photo_archive"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/seventinth_text_size"
                                tools:ignore="MissingPrefix" />

                            <com.smarteist.autoimageslider.SliderView
                                android:id="@+id/photoSlider"
                                android:layout_width="match_parent"
                                android:layout_height="200dp"
                                android:layout_gravity="center"
                                android:layout_marginLeft="16dp"
                                android:layout_marginTop="16dp"
                                android:layout_marginRight="16dp"
                                android:layout_marginBottom="8dp"
                                app:sliderAnimationDuration="600"
                                app:sliderAutoCycleDirection="back_and_forth"
                                app:sliderAutoCycleEnabled="true"
                                app:sliderCircularHandlerEnabled="true"
                                app:sliderIndicatorAnimationDuration="600"
                                app:sliderIndicatorGravity="center_horizontal|bottom"
                                app:sliderIndicatorMargin="6dp"
                                app:sliderIndicatorOrientation="horizontal"
                                app:sliderIndicatorPadding="0dp"
                                app:sliderIndicatorRadius="0dp"
                                app:sliderScrollTimeInSec="1"
                                app:sliderStartAutoCycle="true" />


                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="12dp">

                                <ImageView
                                    android:layout_width="match_parent"
                                    android:layout_height="400dp"
                                    android:layout_centerInParent="true"
                                    android:layout_marginLeft="16dp"
                                    android:layout_marginTop="16dp"
                                    android:layout_marginRight="16dp"
                                    android:scaleType="centerCrop"
                                    android:src="@drawable/qq" />

                                <Button
                                    fontPath="fonts/solaiman_lipi.ttf"
                                    android:layout_width="wrap_content"
                                    android:layout_height="35dp"
                                    android:layout_centerInParent="true"
                                    android:background="@drawable/photo_archive_bg"
                                    android:fontFamily="@font/lato_regular"
                                    android:onClick="showPhotoArchive"
                                    android:paddingLeft="6dp"
                                    android:paddingRight="6dp"
                                    android:text="@string/full_photo_archive"
                                    android:textAllCaps="false"
                                    android:textColor="#fff"
                                    android:textSize="@dimen/seventinth_text_size"
                                    tools:ignore="MissingPrefix" />
                            </RelativeLayout>

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginBottom="8dp"
                        app:cardCornerRadius="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <FrameLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:layout_centerInParent="true">

                                    <com.google.android.exoplayer2.ui.PlayerView
                                        android:id="@+id/playerview"
                                        android:layout_width="match_parent"
                                        android:layout_height="200dp"
                                        android:layout_gravity="center"
                                        app:controller_layout_id="@layout/custom_controls"
                                        app:show_buffering="always">

                                        <ImageView
                                            android:id="@+id/thumbnail"
                                            android:layout_width="match_parent"
                                            android:layout_height="match_parent"
                                            android:background="@drawable/graphic">

                                        </ImageView>

                                        <ImageView
                                            android:id="@+id/playIv"
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:layout_gravity="center"
                                            android:src="@drawable/play"
                                            android:tint="@color/gold_color">

                                        </ImageView>

                                        <ImageView
                                            android:id="@+id/pauseIv"
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:layout_gravity="center"
                                            android:src="@drawable/pause"
                                            android:tint="@color/gold_color"
                                            android:visibility="gone">

                                        </ImageView>

                                        <ImageView
                                            android:id="@+id/replayIv"
                                            android:layout_width="50dp"
                                            android:layout_height="50dp"
                                            android:layout_gravity="center"
                                            android:src="@drawable/ic_replay_black_24dp"
                                            android:tint="@color/gold_color"
                                            android:visibility="gone"/>

                                        <ProgressBar
                                            android:id="@+id/exo_buffering"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:background="@color/gold_color"
                                            android:visibility="invisible"/>

                                    </com.google.android.exoplayer2.ui.PlayerView>


                                </FrameLayout>


                            </RelativeLayout>


                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                android:layout_marginTop="16dp"
                                android:fontFamily="@font/lato_regular"
                                android:text="@string/graphic_novel_mujib"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/seventinth_text_size"
                                android:textStyle="bold"
                                tools:ignore="MissingPrefix" />

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="8dp"
                                android:layout_marginTop="10dp"
                                android:layout_marginRight="8dp"
                                android:layout_marginBottom="8dp"
                                android:fontFamily="@font/lato_regular"
                                android:gravity="center_horizontal"
                                android:lineSpacingExtra="@dimen/line_hide"
                                android:text="@string/grapich_novel_text"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/description_text_size"
                                tools:ignore="MissingPrefix" />

                            <Button
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="100dp"
                                android:layout_height="37dp"
                                android:layout_gravity="center"
                                android:layout_marginTop="8dp"
                                android:layout_marginBottom="16dp"
                                android:background="@drawable/media_btn_bg"
                                android:fontFamily="@font/lato_regular"
                                android:onClick="showGraphicNovel"
                                android:text="@string/view_all"
                                android:textAllCaps="false"

                                android:textColor="@android:color/white"
                                android:textSize="@dimen/description_text_size"
                                tools:ignore="MissingPrefix" />
                        </LinearLayout>


                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="8dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginRight="8dp"
                        android:layout_marginBottom="8dp"
                        app:cardCornerRadius="2dp">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:gravity="center_horizontal"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_width="45dp"
                                    android:layout_height="45dp"
                                    android:layout_marginRight="20dp"
                                    android:src="@drawable/facebook" />

                                <ImageView
                                    android:layout_width="45dp"
                                    android:layout_height="45dp"
                                    android:layout_marginLeft="20dp"
                                    android:src="@drawable/youtube" />

                            </LinearLayout>

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:fontFamily="@font/lato_regular"
                                android:gravity="center_horizontal"
                                android:text="@string/developed_amp_operated_by"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/description_text_size"
                                tools:ignore="MissingPrefix" />

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="8dp"
                                android:fontFamily="@font/lato_regular"
                                android:gravity="center_horizontal"
                                android:text="@string/ict_division"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/hight_text_sizee"
                                tools:ignore="MissingPrefix" />

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="20dp"
                                android:fontFamily="@font/lato_regular"
                                android:gravity="center_horizontal"
                                android:text="@string/powered_by"
                                android:textColor="@android:color/black"
                                android:textSize="@dimen/description_text_size"
                                tools:ignore="MissingPrefix" />

                            <TextView
                                fontPath="fonts/solaiman_lipi.ttf"
